
# Source default screen settings
source .screenrc

# Create shell screens
screen -h 2048  # 0
screen -h 2048  # 1
screen          # 2
screen          # 3
screen          # 4
screen          # 5
screen          # 6
screen          # 7
screen          # 8
screen          # 9
screen          # 10

# Set up screens
at 0 stuff "cd^M"
at 1 stuff "cd^M"
at 10 stuff "cd^M"

# Return to the first screen
select 1
